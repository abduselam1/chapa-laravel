{
    "name": "abduselam/chapa-laravel",
    "description": "A Laravel Package to Accept Payment through Chapa",
    "keywords": [
        "chapa",
        "chapa-laravel"
    ],
    "homepage": "https://github.com/abduselam1/chapa-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kidus Yared",
            "email": "kidusy@chapa.co",
            "role": "Developer"
        }, 
        {
            "name": "Israel Goytom",
            "email": "myfirstname@chapa.co",
            "role": "CTO"
        },
        {
            "name": "Semeredin Nurgeba",
            "email": "semernur11@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Abduselam Hafiz",
            "email": "abduselamhafiz@gmail.com"
        }
    ],
    "require": {
        "php": "7.2 - 8.3 || ^8.3",
        "guzzlehttp/guzzle": "~6|~7|~8|~9",
        "illuminate/support": "5.0 - 11.0 || ~11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "classmap": [
            "src/Chapa.php"
        ],
        "psr-4": {
            "Chapa\\Chapa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chapa\\Chapa\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chapa\\Chapa\\ChapaServiceProvider"
            ],
            "aliases": {
                "Chapa": "Chapa\\Chapa\\Facades\\Chapa"
            }
        }
    }
}
